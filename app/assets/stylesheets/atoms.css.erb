.font-libre {
  font-family: 'Libre Baskerville', serif;
}
* {
  font-family: 'Libre Baskerville', serif;
}
.rad-br1 {
  background-image: url(<%= asset_path 'tody.jpg' %>);
}
.rad-br2 {
  background-image: url(<%= asset_path 'bears.jpg' %>);
}

.fixed-br {
  background-attachment: fixed;
}
.transp-ex-hover {
  opacity: 0.5;
  transition-duration: 1s;
}
.transp-ex-hover:hover {
  opacity: 0.9;
}



.transl-br {
  background-color: rgba(200, 200, 200, 0.3);
}
.overflow {
  scroll: overflow;
}
body, html {
  width: 100%;
  height: 100%;
}
.min-height-100 {
  min-height: 100px;
}
.min-width-50 {
  min-width: 100px;
}
.justify-around {
  justify-content: space-around;
}
.justify-between {
  justify-content: space-between;
}
.display-flex-row {
  display:flex;
  flex-direction: row;
}
.display-block {
  display: block;
}
.black-br {
  background-color: rgba(10, 100, 10, 0.2);
}
.fixed {
  position: fixed;
}
.top {
  top: 0%;
}
.bottom {
  bottom: 0%;
}
.height100{
  height: 100%;
}
.height10{
  height: 10%;
}
.top10 {
  top: 10%;
}
.width-100 { width: 100%;

}

.bordered {
  border: solid 1px black;
}
.hover-chill:hover {
  color: red;
  background-color: green;
}
.right {
  left: 10%;
}
.float-left {
  float: left;
}
.float-right {
  float: right;
}
.clear-left {
  clear: left;
}
.clear-right {
  clear: right;
}
.no-clear {
  clear: none;
}
.margin-1 { margin: 1%;}
.padding-10 { padding: 0% 3%;}
.padding-20 { padding: 0% 10%;}
.no-list-styles {list-style: none;}

#svg-panel {
  min-width: 900px;
  min-height: 700px;
}
svg {
 height: 100%;
  width: 100%;
  min-width: 900px;
  min-height: 700px;
}